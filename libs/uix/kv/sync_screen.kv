 #:kivy 1.11.0
<SyncScreen>:
    name: 'syncscreen'
    BoxLayout
        orientation: 'vertical'
        spacing: dp(5)

        FloatLayout:
            id: box

            MDRoundFlatIconButton:
                text: "Star Sync"
                icon: "download"
                pos_hint: {'center_x': 0.5, 'center_y': 0.6}
                on_release: root.show_example_download_file()
